{
  "name": "blueprint-framework",
  "version": "5.0.0",
  "description": "A modern, powerful, and modular Discord bot framework",
  "main": "build/index.js",
  "repository": "https://github.com/blueprint-js/blueprint.git",
  "author": "Jonathan <axis@axis.ovh>",
  "types": "build/index.d.ts",
  "license": "MPL-2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "blueprint",
    "modern",
    "discord",
    "bot",
    "javascript",
    "typescript",
    "registry",
    "documentation",
    "discord.js-light",
    "log4js"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.45",
    "@types/node": "^14.11.2",
    "@types/ws": "^7.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "lint": "eslint --ext .ts",
    "format": "prettier --write \"./src/**/*.ts\"",
    "prepare": "yarn compile",
    "compile": "swc src -d build && yarn gen-types",
    "gen-types": "tsc -d --declarationDir build --emitDeclarationOnly",
    "fix": "eslint --fix"
  },
  "dependencies": {
    "discord.js-light": "^3.5.4",
    "log4js": "^6.3.0"
  }
}
